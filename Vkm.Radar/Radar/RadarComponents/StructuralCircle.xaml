<UserControl x:Class="Vkm.Radar.Radar.RadarComponents.StructuralCircle"
                             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                             xmlns:viewModel="clr-namespace:Vkm.Radar.Radar.RadarComponents.ViewModel"
                             mc:Ignorable="d"
                             d:DesignHeight="450" d:DesignWidth="450"
                             d:DataContext="{d:DesignInstance Type=viewModel:StructuralCircleComponentViewModel}"
                             x:ClassModifier="internal">
    <Grid>
        <Ellipse x:Name="PART_Ellipse"
                 Stroke="GreenYellow"
                 Width="{Binding Path=Radius}"
                 Height="{Binding Path=Radius}"
                 StrokeThickness="3" />
    </Grid>
</UserControl>